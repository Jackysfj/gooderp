<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- 仓库 -->
        <record id='warehouse_action' model='ir.actions.act_window'>
            <field name='name'>仓库</field>
            <field name='res_model'>warehouse</field>
            <field name='view_mode'>tree</field>
            <field name='domain'>[('type', '=', 'stock'), '|', ('active', '!=', 1), ('active', '=', 1)]</field>
        </record>

        <!-- 出入库单 -->
       <record id='wh_order_out_action' model='ir.actions.act_window'>
            <field name='name'>出库单</field>
            <field name='res_model'>wh.order.out</field>
            <field name='view_mode'>tree,form</field>
            <field name='view_id' ref='wh_order_out_tree' />
        </record>

       <record id='wh_order_in_action' model='ir.actions.act_window'>
            <field name='name'>入库单</field>
            <field name='res_model'>wh.order.in</field>
            <field name='view_mode'>tree,form</field>
            <field name='view_id' ref='wh_order_in_tree' />
        </record>

       <record id='wh_order_internal_action' model='ir.actions.act_window'>
            <field name='name'>出库单</field>
            <field name='res_model'>wh.order.internal</field>
            <field name='view_mode'>tree,form</field>
            <field name='view_id' ref='wh_order_internal_tree' />
        </record>

    </data>
</openerp>
