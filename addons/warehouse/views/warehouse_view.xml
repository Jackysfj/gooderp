<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id='warehouse_tree' model='ir.ui.view'>
            <field name='name'>warehouse.tree</field>
            <field name='model'>warehouse</field>
            <field name='arch' type='xml'>
                <tree string='仓库' editable='bottom'>
                    <field name='code' required='1' />
                    <field name='name' required='1' />
                    <field name='active' widget='boolean_clickable' />
                </tree>
            </field>
        </record>

        <record id='warehouse_action' model='ir.actions.act_window'>
            <field name='name'>仓库</field>
            <field name='res_model'>warehouse</field>
            <field name='view_mode'>tree</field>
            <field name='domain'>['|', ('active', '!=', 1), ('active', '=', 1)]</field>
        </record>

    </data>
</openerp>
